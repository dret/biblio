@article{ken03,
    author =    "Jeff Kenyon",
    journal =   wsj,
    title =     "Web Service Versioning and Deprecation",
    year =      2003,
    month =     feb,
    volume =    3,
    number =    2,
    uri =       "http://www.sys-con.com/webservices/article.cfm?id=467",
