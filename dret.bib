@misc{xmlqdata,
    author =    "Mary F. Fern\'andez and Ashok Malhotra and Jonathan Marsh and Marton Nagy and Norman Walsh",
    title =     "XQuery 1.0 and XPath 2.0 Data Model (XDM)",
    howpublished =  "World Wide Web Consortium, Recommendation REC-xpath-datamodel-20070123",
    month =     jan,
    year =      2007,
    topic =     "xquery[0.9] xpath2[0.9] xdm[1]",
