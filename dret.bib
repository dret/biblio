@misc{xhtml2,
    author =    "Mark Birbeck and Markus Gylling and Shane McCarron and Steven Pemberton",
    title =     "XHTML 2.0",
    howpublished =  "World Wide Web Consortium, Note NOTE-xhtml2-20101216",
    month =     dec,
    year =      2010,
    updates =   "xhtml11",
    topic =     "xml[0.5] html[0.7] xhtml[1]",
    uri =       "http://www.w3.org/TR/2010/NOTE-xhtml2-20101216",
    abstract =  "XHTML 2 is a general-purpose markup language designed to represent documents for a wide range of purposes across the World Wide Web. To this end it does not attempt to be all things to all people, supplying every possible markup idiom, but to supply a generally useful set of elements."
}

@misc{xframes,
    author =    "Steven Pemberton and Masayasu Ishikawa",
    title =     "XFrames",
    howpublished =  "World Wide Web Consortium, Note NOTE-xframes-20101216",
    month =     dec,
    year =      2010,
    uri =       "http://www.w3.org/TR/2010/NOTE-xframes-20101216",
    abstract =  "XFrames is an XML application for composing documents together, replacing HTML Frames. By being a separate application from XHTML, it allows content negotiation to determine if the user agent accepts frames; by encoding the 'population' of frames in the URI, it allows framesets to be bookmarked."
}

