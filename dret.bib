@misc{xptr,
    author =    "Paul Grosso and Eve Maler and Jonathan Marsh and Norman Walsh",
    title =     "XPointer Framework",
    howpublished =  "World Wide Web Consortium, Recommendation REC-xptr-framework-20030325",
