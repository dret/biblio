@string{ibmsystems =     "IBM Systems Journal" }

@article{jac02,
    author =    "Joseph Jacobson and Barrett O. Comiskey and Chris Turner and Jonathan Albert and Perry Tsao",
    title =     "The Last Book",
    journal =   ibmsystems,
    year =      1997,
    volume =    36,
