@misc{svgmobile12,
    author =    {Ola Andersson and Robin Berjon and Erik Dahlstr\"om and Andrew Emmons and Jon Ferraiolo and Anthony Grasso and Vincent Hardy and Scott Hayman and Dean Jackson and Chris Lilley and Cameron McCormack and Andreas Neumann and Craig Northway and Antoine Quint and Nandini Ramani and Doug Schepers and Andrew Shellshear},
