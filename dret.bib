    month =     jan,
    year =      2000,
    topic =     "dt4dtd[1]",
    uri =       "http://www.w3.org/TR/2000/NOTE-dt4dtd-20000113",
    abstract =  "The presented specification allows legacy systems that may presently be unable to convert their DTD markup declarations to XML Schema, to utilize XML Schema conformant datatypes. With it, DTD creators can specify datatypes for attribute values and data content, thereby providing the foundation for a smoother future transition path."
}

@misc{xmlschemaapi,
    author =    "Elena Litani",
    title =     "XML Schema API",
    howpublished =  "World Wide Web Consortium, Member Submission SUBM-xmlschema-api-20040309",
    month =     mar,
    year =      2004,
    topic =     "xsdl[0.9]",
    uri =       "http://www.w3.org/Submission/2004/SUBM-xmlschema-api-20040309",
    abstract =  "This specification defines an XML Schema API, a platform- and language-neutral interface that allows programs and scripts to dynamically access and query the post-schema-validation infoset (PSVI) defined in Contributions to the post-schema-validation infoset (Appendix C.2). This specification also defines interfaces for loading XML schema documents. This specification is implemented in Apache Xerces2 Java Parser. There is also a C++ binding and implementation for this specification in Apache Xerces C++ Parser."
}

@misc{rdql,
