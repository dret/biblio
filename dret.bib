    crossref =  "www2002",
    author =    "Taher H. Haveliwala and Aristades Gionis and Dan Klein and Piotr Indyk",
    title =     "Evaluating Strategies for Similarity Search on the Web",
    pages =     "432-442",
    uri =       "http://www2002.org/CDROM/refereed/75/index.html",
    uri =       "http://dbpubs.stanford.edu/pub/2002-7"
}

