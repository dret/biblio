}

@misc{ilreq,
    author =    "Swaran Lata and Somnath Chandra and Prashant Verma and Richard Ishida",
    title =     "Indic Layout Requirements",
    howpublished =  "World Wide Web Consortium, Working Draft WD-ilreq-20170220",
