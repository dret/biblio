@misc{xmlfragid,
    author =    "Paul Grosso",
    title =     "Proposal for XML Fragment Identifier Syntax 0.9",
    howpublished =  "World Wide Web Consortium, Note NOTE-xml-fragid-20030912",
    month =     sep,
    year =      2003,
    topic =     "xpointer[0.9]",
