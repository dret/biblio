@inproceedings{rob13a,
    crossref =  "balisage2013",
    author =    "Jonathan Robie and Rob Cavicchio and R\'emon Sinnema and Erik Wilde",
    title =     "RESTful Service Description Language (RSDL): Describing RESTful Services Without Tight Coupling",
    topic =     "rsdl[1] rest[0.8]",
    uri =       "http://www.balisage.net/Proceedings/vol10/html/Robie01/BalisageVol10-Robie01.html",
