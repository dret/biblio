@misc{xpath20,
    author =    "Anders Berglund and Scott Boag and Donald D. Chamberlin and Mary F. Fern\'andez and Michael Kay and Jonathan Robie and J\'er\^ome Sim\'eon",
    title =     "XML Path Language (XPath) 2.0",
