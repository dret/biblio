@article{kum04,
    author =    "Ravi Kumar and Jasmine Novak and Prabhakar Raghavan and Andrew Tomkins",
    title =     "Structure and Evolution of Blogspace",
    journal =   cacm,
    year =      2004,
    month =     dec,
    volume =    47,
