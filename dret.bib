@inproceedings{wil96b,
    crossref =  "ecmast96",
    author =    "Erik Wilde and Murali Nanduri and Bernhard Plattner",
    title =     "A Transport-Independent Component for a Group and Session Management Service in Group Communications Platforms",
    pages =     "409-425",
