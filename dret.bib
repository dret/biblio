@misc{csshtml,
    author =    "Tantek \c{C}elik and Bert Bos and Marc Attinasi",
    title =     {Syntax of CSS rules in HTML's "style" attribute},
    howpublished =  "World Wide Web Consortium, Working Draft WD-css-style-attr-20020515",
    month =     may,
    year =      2002,
    topic =     "css[1] html[1]",
    uri =       "http://www.w3.org/TR/2002/WD-css-style-attr-20020515",
    abstract =  {HTML provides a "style" attribute on most elements, to hold a fragment of a style sheet that applies to those elements. One of the possible style sheet languages is CSS. This draft describes the syntax of the CSS fragment that can be used in the "style" attribute.}
}

@misc{css3intro,
    author =    "Eric A. Meyer and Bert Bos",
    title =     "CSS3 Introduction",
    howpublished =  "World Wide Web Consortium, Working Draft WD-css3-roadmap-20010523",
    month =     may,
    year =      2001,
    topic =     "css3[1]",
    uri =       "http://www.w3.org/TR/2001/WD-css3-roadmap-20010523"
}

@misc{css3syntax,
    author =    "L. David Baron",
    title =     "CSS3 module: Syntax",
    howpublished =  "World Wide Web Consortium, Working Draft WD-css3-syntax-20030813",
    month =     aug,
    year =      2003,
    topic =     "css3[1]",
    uri =       "http://www.w3.org/TR/2003/WD-css3-syntax-20030813"
}

@misc{css3preslev,
    author =    "Ian Hickson and H\r{a}kon Wium Lie",
    title =     "CSS3 module: Presentation Levels",
    howpublished =  "World Wide Web Consortium, Working Draft WD-css3-preslev-20030813",
    month =     aug,
    year =      2003,
    topic =     "css3[1]",
    uri =       "http://www.w3.org/TR/2003/WD-css3-preslev-20030813"
}

@misc{css3print,
    editor =    "Melinda Grant",
    title =     "CSS Print Profile",
    howpublished =  "World Wide Web Consortium, Working Draft WD-css-print-20061013",
    month =     oct,
    year =      2006,
    topic =     "css3[1]",
    uri =       "http://www.w3.org/TR/2006/WD-css-print-20061013",
    abstract =  "This specification defines a subset of Cascading Style Sheets Level 2, revision 1 and CSS3 Module: Paged Media for printing to low-cost devices. It is designed for printing in situations where it is not feasible or desirable to install a printer-specific driver, and for situations were some variability in the output is acceptable. This profile is designed to work in conjunction with XHTML-Print and defines a minimum level of conformance as well as an extension set that provides stronger layout control for the printing of mixed text and images, tables and image collections."
}

@misc{becss,
    author =    "Ian Hickson",
    title =     "Behavioral Extensions to CSS",
    howpublished =  "World Wide Web Consortium, Working Draft WD-becss-20070205",
    month =     feb,
    year =      2007,
    topic =     "css3[1]",
    uri =       "http://www.w3.org/TR/2007/WD-becss-20070205",
    abstract =  "Behavioral Extensions provide a way to link to binding technologies, such as XBL, from CSS style sheets. This allows bindings to be selected using the CSS cascade, and thus enables bindings to transparently benefit from the user style sheet mechanism, media selection, and alternate style sheets."
}

@misc{css3ruby,
    author =    "Michel Suignard",
    title =     "CSS3 Module: Ruby",
    howpublished =  "World Wide Web Consortium, Candidate Recommendation CR-css3-ruby-20030514",
    month =     may,
    year =      2003,
    topic =     "css3[1]",
    uri =       "http://www.w3.org/TR/2003/CR-css3-ruby-20030514"
}

@misc{css3ui,
    author =    "Tantek \c{C}elik and Peter Linss and Sho Kuwamoto",
    title =     "User Interface for CSS3",
    howpublished =  "World Wide Web Consortium, Working Draft WD-css3-userint-20000216",
    month =     apr,
    year =      2000,
    topic =     "css3[1]",
    uri =       "http://www.w3.org/TR/2000/WD-css3-userint-20000216"
}

@misc{css3ui2,
    author =    "Tantek \c{C}elik",
    title =     "CSS3 Module: Basic User Interface",
    howpublished =  "World Wide Web Consortium, Candidate Recommendation CR-css3-ui-20040511",
    month =     may,
    year =      2004,
    topic =     "css3[1]",
    updates =   "css3ui",
    uri =       "http://www.w3.org/TR/2004/CR-css3-ui-20040511"
}

@misc{i18nformat,
    editor =    "Marcin Sawicki",
    author =    "Michel Suignard and Takao Suzuki and Chris Wilson",
    title =     "International Layout",
    howpublished =  "World Wide Web Consortium, Working Draft WD-i18n-format-19990910",
    month =     sep,
    year =      1999,
    updates =   "i18nformat",
    uri =       "http://www.w3.org/TR/1999/WD-i18n-format-19990910",
    abstract =  "The HyperText Markup Language (HTML) is a simple markup language used to create hypertext documents that are portable from one platform to another. HTML documents are SGML documents with generic semantics that are appropriate for representing information from a wide range of applications. CSS is a style sheets language that can be applied to HTML to control the style of a document: which fonts and colors to use, how much white space to insert, etc. The following specification extends CSS to support East Asian and Bi-directional text formatting. Familiarity with both CSS 2 and HTML 4.0 is assumed."
}

@misc{css3text,
    author =    "Elika J. Etemad and Paul Nelson",
    title =     "CSS3 Module: Text",
    howpublished =  "World Wide Web Consortium, Working Draft WD-css3-text-20070306",
    month =     mar,
    year =      2007,
    topic =     "css3[1]",
    updates =   "i18nformat",
    uri =       "http://www.w3.org/TR/2007/WD-css3-text-20070306",
    abstract =  "This CSS3 module defines properties for text manipulation and specifies their processing model. It covers line breaking, justification and alignment, white space handling, text decoration and text transformation."
}

@misc{css3page,
    author =    "H\r{a}kon Wium Lie and Melinda Grant",
    title =     "CSS3 Module: Paged Media",
    howpublished =  "World Wide Web Consortium, Working Draft WD-css3-page-20061010",
    month =     oct,
    year =      2006,
    topic =     "css3[1]",
    uri =       "http://www.w3.org/TR/2006/WD-css3-page-20061010",
    abstract =  "This module describes the page model that partitions a flow into pages. It builds on the  Box model module and introduces and defines the page model and paged media. It adds functionality for pagination, page margins, page size and orientation, headers and footers, widows and orphans, and image orientation. Finally it extends generated content to enable page numbering and running headers/footers."
}

@misc{css3gcpm,
    author =    "H\r{a}kon Wium Lie",
    title =     "CSS3 Module: Generated Content for Paged Media",
    howpublished =  "World Wide Web Consortium, Working Draft WD-css3-gcpm-20070504",
    month =     may,
    year =      2007,
    topic =     "css3[1]",
    uri =       "http://www.w3.org/TR/2007/WD-css3-gcpm-20070504",
    abstract =  "This module describes features often used in printed publications. In particular, this specification describes how CSS style sheets can express running headers and footers, leaders, cross-references, footnotes, sidenotes, named flows, hyphenation, new counter styles, character substitution, image resolution, page floats, advanced multi-column layout, conditional content, crop and cross marks, bookmarks, CMYK colors, continuation markers, change bars, line numbers, named page lists, and generated lists. Along with two other CSS3 modules --- multi-column layout and paged media --- this module offers advanced functionality for presenting structured documents on paged media."
}

@misc{css3speech,
    author =    "Dave Raggett and Daniel Glazman and Claudio Santambrogio",
    title =     "CSS3 Speech Module",
    howpublished =  "World Wide Web Consortium, Working Draft WD-css3-speech-20041216",
    month =     dec,
    year =      2004,
    topic =     "css3[1] ssml[0.8]",
    uri =       "http://www.w3.org/TR/2004/WD-css3-speech-20041216",
    abstract =  "CSS (Cascading Style Sheets) is a language for describing the rendering of HTML and XML documents on screen, on paper, in speech, etc. CSS defines aural properties that give control over rendering XML to speech. This draft describes the text to speech properties proposed for CSS level 3. These are designed for match the model described in the Speech Synthesis Markup Language (SSML) Version 1.0."
}

@misc{css3content,
    author =    "Ian Hickson",
    title =     "CSS3 Generated and Replaced Content Module",
    howpublished =  "World Wide Web Consortium, Working Draft WD-css3-content-20030514",
    month =     may,
    year =      2003,
    topic =     "css3[1]",
    uri =       "http://www.w3.org/TR/2003/WD-css3-content-20030514",
    abstract =  "This CSS3 Module describes how to insert and move content around a document, in order to create footnotes, endnotes, section notes. Inserted content can also introduce counters and strings, which can be used for running headers and footers, section numbering, and lists. Finally, techniques for declaring replaced images, as well as scaling and cropping them using CSS, are described."
}

@misc{css3line,
    author =    "Michel Suignard and Eric A. Meyer",
    title =     "CSS3 Module: Line",
    howpublished =  "World Wide Web Consortium, Working Draft WD-css3-linebox-20020515",
    month =     may,
    year =      2002,
    topic =     "css3[1]",
    uri =       "http://www.w3.org/TR/2002/WD-css3-linebox-20020515",
    abstract =  "This document presents a set of CSS line formatting properties. It also includes baseline alignment features as well as related functions like initial line and initial letter effect. It extends on the CSS 2 model."
}

@misc{css3reader,
    author =    "Bert Bos",
    title =     "The CSS 'Reader' Media Type",
    howpublished =  "World Wide Web Consortium, Working Draft WD-css3-reader-20040224",
    month =     feb,
    year =      2004,
    topic =     "css3[1]",
    uri =       "http://www.w3.org/TR/2004/WD-css3-reader-20040224",
    abstract =  {'Reader' is a keyword for use in Media Queries. When a Media Query that includes the 'reader' keyword is attached to (a link to) a style sheet, it indicates that that style sheet is designed to be used by a "reader" device (typically a screen reader), that both displays and speaks a document at the same time. It may also display the document and render it in braille at the same time, or do all three. Media Queries (and thus 'reader') can be used in documents in HTML, XML, SVG, CSS and other formats, wherever they link to a style sheet, and potentially also in links to other resources. (But the latter is not treated in this specification.)}
}

@misc{css3hyperlinks,
    author =    "Tantek \c{C}elik and Bert Bos and Daniel Glazman",
    title =     "CSS3 Hyperlink Presentation Module",
    howpublished =  "World Wide Web Consortium, Working Draft WD-css3-hyperlinks-20040224",
    month =     feb,
    year =      2004,
    topic =     "css3[1]",
    uri =       "http://www.w3.org/TR/2004/WD-css3-hyperlinks-20040224",
    abstract =  "CSS (Cascading Style Sheets) is a simple language for describing the presentation of documents. This specification is a module of level 3 of CSS and contains the functionality required to describe the presentation of hyperlink source anchors and the effects of hyperlink activation."
}

@misc{css3multi,
    author =    "H\r{a}kon Wium Lie",
    title =     "CSS3 Module: Multi-Column Layout",
    howpublished =  "World Wide Web Consortium, Working Draft WD-css3-multicol-20070606",
    month =     jun,
    year =      2007,
    topic =     "css3[1]",
    uri =       "http://www.w3.org/TR/2007/WD-css3-multicol-20070606",
    abstract =  "This module describes multi-column layout in CSS. It builds on the CSS3 Box model module and adds functionality to flow the content of an element into multiple columns."
}

@misc{css3color,
    author =    "Tantek \c{C}elik and Chris Lilley and Steven Pemberton and Brad Pettit",
    title =     "CSS3 Module: Color",
    howpublished =  "World Wide Web Consortium, Candidate Recommendation CR-css3-color-20030514",
    month =     may,
    year =      2003,
    topic =     "css3[1]",
    uri =       "http://www.w3.org/TR/2003/CR-css3-color-20030514",
    abstract =  "CSS (Cascading Style Sheets) is a language for describing the rendering of HTML and XML documents on screen, on paper, in speech, etc. To color elements in a document, it uses color related properties and respective values. This specification describes the properties and values that are proposed for CSS level 3. It includes and extends them from properties and values of CSS level 2."
}

@misc{css3bg,
    author =    "Tim Boland and Bert Bos",
    title =     "CSS3 Module: Backgrounds",
    howpublished =  "World Wide Web Consortium, Working Draft WD-css3-background-20020802",
    month =     aug,
    year =      2002,
    topic =     "css3[1]",
    uri =       "http://www.w3.org/TR/2002/WD-css3-background-20020802",
    abstract =  "CSS (Cascading Style Sheets) is a language for describing the rendering of structured documents (such as HTML and XML) on screen, on paper, in speech, etc. This draft describes the functionality that is proposed for CSS level 3 to describe backgrounds, such as background colors and background images. It includes and extends the functionality of CSS level 2, which builds on CSS Level 1."
}

@misc{css3background,
    author =    "Tim Boland and Bert Bos",
    title =     "CSS3 Backgrounds and Borders Module",
    howpublished =  "World Wide Web Consortium, Working Draft WD-css3-background-20050216",
    month =     feb,
    year =      2005,
    topic =     "css3[1]",
    updates =   "css3border css3bg",
    uri =       "http://www.w3.org/TR/2005/WD-css3-background-20050216",
    abstract =  "CSS (Cascading Style Sheets) is a language for describing the rendering of structured documents (such as HTML and XML) on screen, on paper, in speech, etc. This draft contains the proposed functionality for CSS level 3 to describe borders and backgrounds, including borders consisting of images and backgrounds with multiple images. It includes and extends the functionality of CSS level 2, which builds on CSS level 1. This module replaces two earlier drafts: CSS3 Backgrounds and CSS3 Border."
}

@misc{css3sel,
    author =    "Daniel Glazman and Tantek \c{C}elik and Ian Hickson and Peter Linss and John Williams",
    title =     "CSS3 Module: Selectors",
    howpublished =  "World Wide Web Consortium, Working Draft WD-css3-selectors-20051215",
    month =     dec,
    year =      2005,
    topic =     "css3[1]",
    uri =       "http://www.w3.org/TR/2005/WD-css3-selectors-20051215",
    abstract =  "Selectors are patterns that match against elements in a tree. Selectors have been optimized for use with HTML and XML, and are designed to be usable in performance-critical code. CSS (Cascading Style Sheets) is a language for describing the rendering of HTML and XML documents on screen, on paper, in speech, etc. CSS uses Selectors for binding style properties to elements in the document. This document describes extensions to the selectors defined in CSS level 2. These extended selectors will be used by CSS level 3. Selectors define the following function: expression * element -> boolean. That is, given an element and a selector, this specification defines whether that element matches the selector. These expressions can also be used, for instance, to select a set of elements, or a single element from a set of elements, by evaluating the expression across all the elements in a subtree. STTS (Simple Tree Transformation Sheets), a language for transforming XML trees, uses this mechanism."
}

@misc{selectorsapi,
    author =    "Anne van Kesteren",
    title =     "Selectors API",
    howpublished =  "World Wide Web Consortium, Working Draft WD-selectors-api-20060926",
    month =     sep,
    year =      2006,
    topic =     "css3[0.9] dom[0.9]",
    uri =       "http://www.w3.org/TR/2006/WD-selectors-api-20060926",
    abstract =  "This specification defines two methods for retrieving Element nodes in the DOM using a group of selectors as defined in CSS3 Selectors."
}

@misc{css3cas,
    author =    "H\r{a}kon Wium Lie",
    title =     "CSS3 Module: Cascading and Inheritance",
    howpublished =  "World Wide Web Consortium, Working Draft WD-css3-cascade-20051215",
    month =     dec,
    year =      2005,
    topic =     "css3[1]",
    uri =       "http://www.w3.org/TR/2005/WD-css3-cascade-20051215",
    abstract =  {This CSS3 module describes how values are assigned to properties. CSS allows several style sheets to influence the rendering of a document, and the process of combining these style sheets is called "cascading". If no value can be found through cascading, a value can be inherited from the parent element or the property's initial value is used.}
}

@misc{css3layout,
    author =    "Bert Bos",
    title =     "CSS Advanced Layout Module",
    howpublished =  "World Wide Web Consortium, Working Draft WD-css3-layout-20070809",
    month =     aug,
    year =      2007,
    topic =     "css3[1]",
    uri =       "http://www.w3.org/TR/2007/WD-css3-layout-20070809",
    abstract =  {CSS is a simple, declarative language for creating style sheets that specify the rendering of HTML and other structured documents. This specification is part of level 3 of CSS ("CSS3") and contains features to describe layouts at a high level, meant for tasks such as the positioning and alignment of "widgets" in a graphical user interface or the layout grid for a page or a window, in particular when the desired visual order is different from the order of the elements in the source document. Other CSS3 modules contain properties to specify fonts, colors, text alignment, list numbering, tables, etc. The features in this module are described together for easier reading, but are usually not implemented as a group. CSS3 modules often depend on other modules or contain features for several media types. Implementers should look at the various "profiles" of CSS, which list consistent sets of features for each type of media.}
}

@misc{css3grid,
    author =    "Alex Mogilevsky and Markus Mielke",
    title =     "CSS Grid Positioning Module Level 3",
    howpublished =  "World Wide Web Consortium, Working Draft WD-css3-grid-20070905",
    month =     sep,
    year =      2007,
    topic =     "css3[1]",
    uri =       "http://www.w3.org/TR/2007/WD-css3-grid-20070905",
    abstract =  "This module describes integration of grid-based layout (similar to the grids traditionally used in books and newspapers) with CSS sizing and positioning."
}

@misc{css3val,
    author =    "H\r{a}kon Wium Lie and Chris Lilley",
    title =     "CSS3 Values and Units",
    howpublished =  "World Wide Web Consortium, Working Draft WD-css3-values-20060919",
    month =     sep,
    year =      2006,
    topic =     "css3[1]",
    uri =       "http://www.w3.org/TR/2006/WD-css3-values-20060919",
    abstract =  {This CSS3 module describes the various values and units that CSS properties accept. Also, it describes how values are computed from "specified" (which is what the cascading process yields) through "computed" and "used" into "actual" values. The main purpose of this module is to define common values and units in one specification which can be referred to by other modules. As such, it does not make sense to claim conformance with this module alone.}
}

@misc{css3box,
    author =    "Bert Bos",
    title =     "CSS Basic Box Model",
    howpublished =  "World Wide Web Consortium, Working Draft WD-css3-box-20070809",
    month =     aug,
    year =      2007,
    topic =     "css3[1]",
    uri =       "http://www.w3.org/TR/2007/WD-css3-box-20070809",
    abstract =  {CSS (Cascading Style Sheets) describes the rendering of documents on various media. When textual documents (e.g., HTML) are laid out on visual media (e.g., screen or print), CSS models the document as a hierarchy of boxes containing words, lines, paragraphs, tables, etc. each with properties such as size, color and font. This module describes the basic types of boxes, with their padding and margin, and the normal "flow" (i.e., the sequence of blocks of text with margins in-between). It also defines "floating" boxes, but other kinds of layout, such as tables, absolute positioning, ruby annotations, grid layouts, columns and numbered pages, are described by other modules. Also, the layout of text inside each line (including the handling of left-to-right and right-to-left scripts) is defined elsewhere. Boxes may contain either horizontal or vertical lines of text. Boxes of different orientations may be mixed in one flow. (This is a level 3 feature.) }
}

@misc{css3webfonts,
    author =    "Michel Suignard and Chris Lilley",
    title =     "CSS3 Module: Web Fonts",
    howpublished =  "World Wide Web Consortium, Working Draft WD-css3-webfonts-20020802",
    month =     aug,
    year =      2002,
    topic =     "css3[1]",
    uri =       "http://www.w3.org/TR/2002/WD-css3-webfonts-20020802",
    abstract =  "This document presents a set of properties allowing font description by a user agent. This specification is very close to the similar section in CSS 2. Only errata have been applied."
}

@misc{css3fonts,
    author =    "Michel Suignard and Chris Lilley",
    title =     "CSS3 Module: Fonts",
    howpublished =  "World Wide Web Consortium, Working Draft WD-css3-fonts-20020802",
    month =     aug,
    year =      2002,
    topic =     "css3[1]",
    uri =       "http://www.w3.org/TR/2002/WD-css3-fonts-20020802",
    abstract =  "This document presents a set of properties allowing font specification by a user agent as well as additional font decoration properties like font effects, emphasis, smoothing, etc. The font specification is very close to the similar section in CSS 2. The font decoration properties are new to CSS3."
}

@misc{css3lists,
    author =    "Ian Hickson and Tantek \c{C}elik",
    title =     "CSS3 Module: Lists",
    howpublished =  "World Wide Web Consortium, Working Draft WD-css3-lists-20021107",
    month =     nov,
    year =      2002,
    topic =     "css3[1]",
    uri =       "http://www.w3.org/TR/2002/WD-css3-lists-20021107",
    abstract =  "This CSS level 3 module describes how lists are styled."
}

@misc{css3border,
    author =    "Tapas Roy",
    title =     "CSS3 Module: Border",
    howpublished =  "World Wide Web Consortium, Working Draft WD-css3-border-20021107",
    month =     nov,
    year =      2002,
    topic =     "css3[1]",
    uri =       "http://www.w3.org/TR/2002/WD-css3-border-20021107",
    abstract =  "CSS (Cascading Style Sheets) is a language for describing the rendering of structured documents (such as HTML and XML) on screen, on paper, in speech, etc. This draft describes the functionality that is proposed for CSS level 3 to describe borders, such as border styles, border colors and border images. It includes and extends the functionality of CSS level 2, which builds on CSS Level 1."
}

@misc{css3namespace,
    author =    "Elika J. Etemad",
    title =     "CSS3 Module: Namespaces",
    howpublished =  "World Wide Web Consortium, Working Draft WD-css3-namespace-20060828",
    month =     aug,
    year =      2006,
    topic =     "css3[1]",
    uri =       "http://www.w3.org/TR/2006/WD-css3-namespace-20060828",
    abstract =  "This CSS module defines the syntax for using namespaces in CSS. It introduces the @namespace rule for declaring the default namespace and binding namespaces to namespace prefixes, and it defines a syntax that other specifications can adopt for using those prefixes in namespace-qualified names."
}

@misc{cpcreq,
    author =    "Roger Gimson and Lalitha Suryanarayana and Markus Lauff",
    title =     "Core Presentation Characteristics: Requirements and Use Cases",
    howpublished =  "World Wide Web Consortium, Working Draft WD-cpc-req-20030510",
    month =     may,
    year =      2003,
    uri =       "http://www.w3.org/TR/2003/WD-cpc-req-20030510"
}

@misc{xsdversioningusecases,
    author =    "Hoylen Sue",
    title =     "XML Schema Versioning Use Cases",
    howpublished =  "World Wide Web Consortium, XML Schema Working Group",
    month =     jan,
    year =      2006,
    uri =       "http://www.w3.org/XML/2005/xsd-versioning-use-cases/2006-01-31.html",
    topic =     "xsdl[0.8]",
    abstract =  "This document describes use cases where XML Schemas are being versioned. These are situations when there are more than one XML Schema, and those schemas are related to each other in some way. The use cases describe the desired behaviour from XML Schema processors when they encounter the different versions of schemas and the instances defined by them. This document has been produced by the W3C XML Schema Working Group, to serve as input to the Working Group's work on the versioning of XML Schemas. It is used to define what types of versioning situations that XML Schema should addressing. The use cases illustrate the types of versioning problems that should be solved by versioning mechanisms that might be added to XML Schema. XML Schema might not be able to solve all the use cases, but it is hoped that it can solve a majority of them."
}

@misc{xmlversioning,
    author =    "David Orchard",
    title =     "Extending and Versioning Languages Part 1",
    howpublished =  "World Wide Web Consortium, Draft TAG Finding",
    month =     mar,
    year =      2007,
    uri =       "http://www.w3.org/2001/tag/doc/versioning-20070326.html",
    topic =     "xml[0.8]",
    abstract =  "This document provides terminology for discussing language versioning, a number of questions that language designers must answer, and a variety of version identification strategies. A separate document contains schema language specific discussion."
}

@misc{xmlschemaguide2versioning,
    author =    "David Orchard",
    title =     "Guide to Versioning XML Languages using new XML Schema 1.1 features",
    howpublished =  "World Wide Web Consortium, Working Draft WD-xmlschema-guide2versioning-20070720",
    month =     jul,
    year =      2007,
    uri =       "http://www.w3.org/TR/2007/WD-xmlschema-guide2versioning-20070720",
    topic =     "xml[0.7] xsdl[0.9]",
    updates =   "xmlversioning",
    abstract =  "This document is a guide to versioning XML languages using new XML Schema 1.1 mechanisms. It shows many of the new Schema 1.1 mechanisms, provides context above the schema 1.1 WD, and solicits reader input."
}

@misc{nsdocuments,
    author =    "Norman Walsh",
    title =     "Associating Resources with Namespaces",
    howpublished =  "World Wide Web Consortium, Draft TAG Finding",
    month =     dec,
    year =      2005,
    uri =       "http://www.w3.org/2001/tag/doc/nsDocuments-2005-12-13",
    topic =     "xmlns[0.9] rddl[0.8] grddl[0.8]",
    abstract =  "This Finding addresses the question of how ancillary information (schemas, stylesheets, documentation, etc.) can be associated with a namespace."
}

@misc{html32,
    author =    "Dave Raggett",
    title =     "HTML 3.2 Reference Specification",
    howpublished =  "World Wide Web Consortium, Recommendation REC-html32",
    month =     jan,
    year =      1997,
    topic =     "html[1]",
    updates =   "rfc1866",
    uri =       "http://www.w3.org/TR/REC-html32",
    abstract =  "This document has been reviewed by W3C members and other interested parties and has been endorsed by the Director as a W3C Recommendation. It is a stable document and may be used as reference material or cited as a normative reference from another document. W3C's role in making the Recommendation is to draw attention to the specification and to promote its widespread deployment. This enhances the functionality and interoperability of the Web."
}

@misc{html40,
    author =    "Dave Raggett and Arnaud {Le Hors} and Ian Jacobs",
    title =     "HTML 4.0 Specification",
    howpublished =  "World Wide Web Consortium, Recommendation REC-html40-19980424",
    month =     apr,
    year =      1998,
    topic =     "html[1]",
    updates =   "html32",
    uri =       "http://www.w3.org/TR/1998/REC-html40-19980424",
    abstract =  "This specification defines the HyperText Markup Language (HTML), version 4.0, the publishing language of the World Wide Web. In addition to the text, multimedia, and hyperlink features of the previous versions of HTML, HTML 4.0 supports more multimedia options, scripting languages, style sheets, better printing facilities, and documents that are more accessible to users with disabilities. HTML 4.0 also takes great strides towards the internationalization of documents, with the goal of making the Web truly World Wide. HTML 4.0 is an SGML application conforming to International Standard ISO 8879 --- Standard Generalized Markup Language."
}

@misc{html401,
    author =    "Dave Raggett and Arnaud {Le Hors} and Ian Jacobs",
    title =     "HTML 4.01 Specification",
    howpublished =  "World Wide Web Consortium, Recommendation REC-html401-19991224",
    month =     dec,
    year =      1999,
    topic =     "html[1]",
    updates =   "html40",
    uri =       "http://www.w3.org/TR/1999/REC-html401-19991224",
    abstract =  "This specification defines the HyperText Markup Language (HTML), the publishing language of the World Wide Web. This specification defines HTML 4.01, which is a subversion of HTML 4. In addition to the text, multimedia, and hyperlink features of the previous versions of HTML (HTML 3.2 and HTML 2.0), HTML 4 supports more multimedia options, scripting languages, style sheets, better printing facilities, and documents that are more accessible to users with disabilities. HTML 4 also takes great strides towards the internationalization of documents, with the goal of making the Web truly World Wide. HTML 4 is an SGML application conforming to International Standard ISO 8879 --- Standard Generalized Markup Language."
}

@misc{xhtml10,
    author =    "Steven Pemberton",
    title =     "XHTML 1.0: The Extensible HyperText Markup Language",
    howpublished =  "World Wide Web Consortium, Recommendation REC-xhtml1-20000126",
    month =     jan,
    year =      2000,
    topic =     "xml[0.5] html[0.7] xhtml[1]",
    uri =       "http://www.w3.org/TR/2000/REC-xhtml1-20000126",
    abstract =  "This specification defines XHTML 1.0, a reformulation of HTML 4 as an XML 1.0 application, and three DTDs corresponding to the ones defined by HTML 4. The semantics of the elements and their attributes are defined in the W3C Recommendation for HTML 4. These semantics provide the foundation for future extensibility of XHTML. Compatibility with existing HTML user agents is possible by following a small set of guidelines."
}

@misc{xhtml10sec,
    author =    "Steven Pemberton",
    title =     "XHTML 1.0: The Extensible HyperText Markup Language (Second Edition)",
    howpublished =  "World Wide Web Consortium, Recommendation REC-xhtml1-20020801",
    month =     aug,
    year =      2002,
    topic =     "xml[0.5] html[0.7] xhtml[1]",
    updates =   "xhtml10",
    uri =       "http://www.w3.org/TR/2002/REC-xhtml1-20020801",
    abstract =  "This specification defines the Second Edition of XHTML 1.0, a reformulation of HTML 4 as an XML 1.0 application, and three DTDs corresponding to the ones defined by HTML 4. The semantics of the elements and their attributes are defined in the W3C Recommendation for HTML 4. These semantics provide the foundation for future extensibility of XHTML. Compatibility with existing HTML user agents is possible by following a small set of guidelines."
}

@misc{xhtmlbasic,
    author =    "Mark A. Baker and Masayasu Ishikawa and Shinichi Matsui and Peter Stark and Ted Wugofski and Toshihiko Yamakami",
    title =     "XHTML Basic",
    howpublished =  "World Wide Web Consortium, Recommendation REC-xhtml-basic-20001219",
    month =     dec,
    year =      2000,
    topic =     "xml[0.5] html[0.7] xhtml[1]",
    uri =       "http://www.w3.org/TR/2000/REC-xhtml-basic-20001219",
    abstract =  {The XHTML Basic document type includes the minimal set of modules required to be an XHTML host language document type, and in addition it includes images, forms, basic tables, and object support. It is designed for Web clients that do not support the full set of XHTML features; for example, Web clients such as mobile phones, PDAs, pagers, and settop boxes. The document type is rich enough for content authoring. XHTML Basic is designed as a common base that may be extended. For example, an event module that is more generic than the traditional HTML 4 event system could be added or it could be extended by additional modules from XHTML Modularization such as the Scripting Module. The goal of XHTML Basic is to serve as a common language supported by various kinds of user agents. The document type definition is implemented using XHTML modules as defined in "Modularization of XHTML".}
}

@misc{xhtmlbasic11,
    author =    "Shane McCarron and Masayasu Ishikawa",
    title =     "XHTML Basic 1.1",
    howpublished =  "World Wide Web Consortium, Candidate Recommendation CR-xhtml-basic-20070713",
    month =     jul,
    year =      2007,
    topic =     "xml[0.5] html[0.7] xhtml[1]",
    updates =   "xhtmlbasic",
    uri =       "http://www.w3.org/TR/2007/CR-xhtml-basic-20070713",
    abstract =  {The XHTML Basic document type includes the minimal set of modules required to be an XHTML host language document type, and in addition it includes images, forms, basic tables, and object support. It is designed for Web clients that do not support the full set of XHTML features; for example, Web clients such as mobile phones, PDAs, pagers, and settop boxes. The document type is rich enough for content authoring. XHTML Basic is designed as a common base that may be extended. The goal of XHTML Basic is to serve as a common language supported by various kinds of user agents. This revision, 1.1, supercedes version 1.0 as defined in http://www.w3.org/TR/2000/REC-xhtml-basic-20001219. In this revision, several new features have been incorporated into the language in order to better serve the small-device community that is this language's major user: 1. XHTML Forms; 2. Intrinsic Events; 3. The value attribute for the li element; 4. The target attribute; 5. The style element; 6. The style attribute; 7. XHTML Presentation module; 8. The inputmode attribute. The document type definition is implemented using XHTML modules as defined in "XHTML Modularization".}
}

@misc{xhtml11,
    author =    "Murray Altheim and Shane McCarron",
    title =     "XHTML 1.1 --- Module-based XHTML",
    howpublished =  "World Wide Web Consortium, Recommendation REC-xhtml11-20010531",
    month =     may,
    year =      2001,
    topic =     "xml[0.5] html[0.7] xhtml[1]",
    updates =   "xhtml10",
    uri =       "http://www.w3.org/TR/2001/REC-xhtml11-20010531",
    abstract =  "This Recommendation defines a new XHTML document type that is based upon the module framework and modules defined in Modularization of XHTML. The purpose of this document type is to serve as the basis for future extended XHTML 'family' document types, and to provide a consistent, forward-looking document type cleanly separated from the deprecated, legacy functionality of HTML 4 that was brought forward into the XHTML 1.0 document types. This document type is essentially a reformulation of XHTML 1.0 Strict using XHTML Modules. This means that many facilities available in other XHTML Family document types (e.g., XHTML Frames) are not available in this document type. These other facilities are available through modules defined in Modularization of XHTML, and document authors are free to define document types based upon XHTML 1.1 that use these facilities."
}

@misc{xhtml11sec,
    author =    "Murray Altheim and Shane McCarron",
    title =     "XHTML 1.1 --- Module-based XHTML --- Second Edition",
    howpublished =  "World Wide Web Consortium, Wokring Draft WD-xhtml11-20070216",
    month =     feb,
    year =      2007,
    topic =     "xml[0.5] html[0.7] xhtml[1]",
    updates =   "xhtml11",
    uri =       "http://www.w3.org/TR/2007/WD-xhtml11-20070216",
    abstract =  "This document defines an XHTML document type that is based upon the module framework and modules defined in XHTML Modularization. The purpose of this document type is to serve as the basis for future extended XHTML 'family' document types, and to provide a consistent, forward-looking document type cleanly separated from the deprecated, legacy functionality of HTML 4 that was brought forward into the XHTML 1.0 document types. This document type is most similar to XHTML 1.0 Strict, built using XHTML Modules. This means that many facilities available in other XHTML Family document types (e.g., XHTML Frames) are not available in this document type. These other facilities are available through modules defined in XHTML Modularization, and document authors are free to define document types based upon XHTML 1.1 that use these facilities (see for information on creating new document types)."
}

@misc{xhtmlbuild,
    author =    "Murray Altheim and Shane McCarron",
    title =     "Building XHTML Modules",
    howpublished =  "World Wide Web Consortium, Working Draft WD-xhtml-building-20000105",
    month =     jan,
    year =      2000,
    topic =     "xml[0.5] html[0.7] xhtml[1]",
    uri =       "http://www.w3.org/TR/2000/WD-xhtml-building-20000105",
    abstract =  "This working draft defines the mechanism for defining markup language modules that are compatible with the modularization framework used by XHTML. This includes a definition of the way in which an abstract module is specified, the way in which this abstraction is mapped into an XML DTD, and the way in which the resulting DTD module can be combined with other XHTML DTD modules to create new markup languages. In the future, it is expected that instructions will also be provided for mapping the abstract specifications into an XML Schema. Note that the materials in this document were formerly part of the Modularization of XHTML document, but have been separated out for editorial purposes."
}

@misc{xhtmlmod,
    author =    "Murray Altheim and Frank Boumphrey and Sam Dooley and Shane McCarron and Sebastian Schnitzenbaumer and Ted Wugofski",
    title =     "Modularization of XHTML",
    howpublished =  "World Wide Web Consortium, Recommendation REC-xhtml-modularization-20010410",
    month =     apr,
    year =      2001,
    topic =     "xml[0.5] html[0.7] xhtml[1]",
    updates =   "xhtmlbuild",
    uri =       "http://www.w3.org/TR/2001/REC-xhtml-modularization-20010410",
    abstract =  "This Recommendation specifies an abstract modularization of XHTML and an implementation of the abstraction using XML Document Type Definitions (DTDs). This modularization provides a means for subsetting and extending XHTML, a feature needed for extending XHTML's reach onto emerging platforms."
}

@misc{xhtmlmodsch,
    author =    "Daniel Austin and Subramanian Peruvemba and Shane McCarron and Masayasu Ishikawa",
    title =     "Modularization of XHTML in XML Schema",
    howpublished =  "World Wide Web Consortium, Working Draft WD-xhtml-m12n-schema-20031003",
    month =     oct,
    year =      2003,
    topic =     "xhtml[0.9] xsdl[0.7]",
    uri =       "http://www.w3.org/TR/2003/WD-xhtml-m12n-schema-20031003",
    abstract =  "This document describes a methodology for modularization of XHTML using XML Schema. The document also provides a XML Schema implementation of the abstract modules defined in Modularization of XHTML."
}

@misc{xhtmlmodsec,
    author =    "Daniel Austin and Subramanian Peruvemba and Shane McCarron and Masayasu Ishikawa and Mark Birbeck",
    title =     "XHTML Modularization 1.1",
    howpublished =  "World Wide Web Consortium, Working Draft WD-xhtml-modularization-20060705",
    month =     jul,
    year =      2006,
    topic =     "xml[0.5] html[0.7] xhtml[1]",
    updates =   "xhtmlmod xhtmlmodsch",
    uri =       "http://www.w3.org/TR/2006/WD-xhtml-modularization-20060705",
    abstract =  "This document is version 1.1 of XHTML Modularization, an abstract modularization of XHTML and implementations of the abstraction using XML Document Type Definitions (DTDs), and XML Schemas. This modularization provides a means for subsetting and extending XHTML, a feature needed for extending XHTML's reach onto emerging platforms. This specification is intended for use by language designers as they construct new XHTML Family Markup Languages. This specification does not define the semantics of elements and attributes, only how those elements and attributes are assembled into modules, and from those modules into markup languages. This second version of this specification includes several minor updates to provide clarifications and address errors found in the first version. It also provides an implementation using XML Schemas."
}

@misc{xhtmlev,
    author =    "Shane McCarron and T. V. Raman",
    title =     "XHTML Events ---  An updated events syntax for XHTML and friends",
    howpublished =  "World Wide Web Consortium, Working Draft WD-xhtml-events-20010608",
    month =     jun,
    year =      2001,
    topic =     "xhtml[0.9]",
    uri =       "http://www.w3.org/TR/2001/WD-xhtml-events-20010608",
    abstract =  "The XHTML Events module defined in this specification provides XHTML host languages the ability to uniformly integrate event listeners and associated event handlers with Document Object Model (DOM) Level 2 event interfaces. The result is to provide XHTML based languages an interoperable way of associating behaviors with document-level markup. In addition, this specification defines a subset of the XHTML Events module called basic events for use on simpler client devices. Finally, the XHTML Event Types Module defines the XHTML language event types."
}

@misc{xhtmlrole,
    author =    "Mark Birbeck and Shane McCarron and Steven Pemberton and T. V. Raman and Richard Schwerdtfeger",
    title =     "XHTML Role Attribute Module",
    howpublished =  "World Wide Web Consortium, Working Draft WD-xhtml-role-20061113",
    month =     nov,
    year =      2006,
    topic =     "xhtml[0.9]",
    uri =       "http://www.w3.org/TR/2006/WD-xhtml-role-20061113",
    abstract =  "The XHTML Role Attribute defined in this specification allows the author to annotate XML Languages with machine-extractable semantic information about the purpose of an element. Use cases include accessibility, device adaptation, server-side processing, and complex data description. This attribute can be integrated into any markup language based upon XHTML Modularization."
}

@misc{xhtmlmathmlsvg,
    author =    "Masayasu Ishikawa",
    title =     "An XHTML + MathML + SVG Profile",
    howpublished =  "World Wide Web Consortium, Working Draft WD-XHTMLplusMathMLplusSVG-20020809",
    month =     aug,
    year =      2002,
    topic =     "xhtml[0.8] mathml[0.8] svg[0.8]",
    uri =       "http://www.w3.org/TR/2002/WD-XHTMLplusMathMLplusSVG-20020809",
    abstract =  "An XHTML+MathML+SVG profile is a profile that combines XHTML 1.1, MathML 2.0, and SVG 1.1 together. This profile enables mixing XHTML, MathML and SVG in the same document using XML namespaces mechanism, while allowing validation of such a mixed-namespace document. An XHTML 1.1 + MathML 2.0 + SVG 1.1 DTD driver is provided. An XHTML version of this document is conforming to this DTD."
}

@misc{xhtmlprint,
    author =    "Melinda Grant and Jim Bigelow",
    title =     "XHTML-Print",
    howpublished =  "World Wide Web Consortium, Recommendation REC-xhtml-print-20060920",
    month =     sep,
    year =      2006,
    topic =     "xhtml[0.8]",
    uri =       "http://www.w3.org/TR/2006/REC-xhtml-print-20060920",
    abstract =  "XHTML-Print is member of the family of XHTML languages defined by the Modularization of XHTML. It is designed to be appropriate for printing from mobile devices to low-cost printers that might not have a full-page buffer and that generally print from top-to-bottom and left-to-right with the paper in a portrait orientation. XHTML-Print is also targeted at printing in environments where it is not feasible or desirable to install a printer-specific driver and where some variability in the formatting of the output is acceptable."
}

@misc{xmlmediatypes,
    author =    {Anish Karmarkar and \"Umit Yal\c{c}\i{}nalp},
    title =     "Assigning Media Types to Binary Data in XML",
    howpublished =  "World Wide Web Consortium, Note NOTE-xml-media-types-20050504",
    month =     may,
    year =      2005,
    topic =     "xml[0.8] mime[0.8]",
    uri =       "http://www.w3.org/TR/2005/NOTE-xml-media-types-20050504",
    abstract =  "This document addresses the need to indicate the media type associated with binary element content in an XML document and the need to specify, in XML Schema, the expected media type(s) associated with binary element content. It is expected that the additional information about the media type will be used for optimizing the handling of binary data that is part of a Web services message."
}

@misc{xhtmlmediatypes,
