@string{dsonline = "IEEE Distributed Systems Online" }

@article{got07,
    author =    "Greg Goth",
    journal =   dsonline,
    title =     "Opening the Mobile Net",
    year =      2007,
    month =     nov,
    volume =    8,
    number =    11,
    doi =       "10.1109/MDSO.2007.64",
    uri =       "http://www.firstmonday.org/issues/issue11_10/duguid/",
    abstract =  {iPhone hackers, spectrum auctions, new carrier technology might doom mobile network carriers' "garden wall" restrictions on consumer choices.}
}

