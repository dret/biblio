@inproceedings{hal02b,
    crossref =  "compdoc02",
    title =     "Eclipse Help System: An Open Source User Assistance Offering",
    author =    "Kari L. Halsted and James H. Roberts",
    pages =     "49-59",
    topic =     "eclipse[0.8]",
