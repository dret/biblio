@misc{xforms11req,
    author =    "John M. Boyer and Roland Merrick",
    title =     "XForms 1.1 Requirements",
    howpublished =  "World Wide Web Consortium, Note NOTE-xforms-11-req-20040831",
    month =     aug,
    year =      2004,
    topic =     "xforms[1]",
    updates =   "xforms10",
    uri =       "http://www.w3.org/TR/2004/NOTE-xforms-11-req-20040831",
    abstract =  "XForms is an XML application that represents the next generation of forms for the Web. This document specifies the requirements for XForms 1.1."
