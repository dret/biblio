    month =     oct,
    year =      2017,
    topic =     "dom3[0.9]",
    updates =   "pagevisibility2nd",
    uri =       "http://www.w3.org/TR/2017/PR-page-visibility-2-20171017",
    abstract =  "This specification defines a means to programmatically determine the visibility state of a document. This can aid in the development of power and CPU efficient web applications."
}

@misc{performancetimeline,
    author =    "Jatinder Mann and Zhiheng Wang",
    title =     "Performance Timeline",
    howpublished =  "World Wide Web Consortium, Recommendation REC-performance-timeline-20131212",
    month =     dec,
    year =      2013,
    topic =     "dom3[0.9]",
    uri =       "http://www.w3.org/TR/2013/REC-performance-timeline-20131212",
