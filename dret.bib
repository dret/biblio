@misc{pushapi,
    author =    "Peter Beverloo and Martin Thomson",
    title =     "Push API",
    howpublished =  "World Wide Web Consortium, Working Draft WD-push-api-20171215",
    month =     dec,
    year =      2017,
    topic =     "html[0.9] http[0.8] dom[0.8]",
