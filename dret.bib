}

@inproceedings{eir04,
    crossref =  "www2004",
    author =    "Nadav Eiron and Kevin S. McCurley and John A. Tomlin",
    title =     "Ranking the Web Frontier",
    pages =     "309-318",
    topic =     "pagerank[0.8]",
    uri =       "http://www2004.org/proceedings/docs/1p309.pdf"
