@inproceedings{etz04a,
    crossref =  "www2004",
    author =    "Oren Etzioni and Michael Cafarella and Doug Downey and Stanley Kok and Ana-Maria Popescu and Tal Shaked and Stephen Soderland and Daniel S. Weld and Alexander Yates",
