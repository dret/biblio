}

@misc{jlreq20,
    author =    "Hiroyuki Chiba and Junsaburo Edamoto and Richard Ishida and Keiichiro Ishino and Seiichi Kato and Tatsuo Kobayashi and Toshi Kobayashi and Kenzou Onozawa and Felix Sasaki and Hajime Shiozawa",
