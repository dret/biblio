@string{wsj = "Web Services Journal" }

@article{ken03,
    author =    "Jeff Kenyon",
    journal =   wsj,
    title =     "Web Service Versioning and Deprecation",
    year =      2003,
    month =     feb,
    volume =    3,
    number =    2,
    uri =       "http://www.sys-con.com/webservices/article.cfm?id=467",
    uri =       "http://www.findarticles.com/p/articles/mi_m0MLV/is_2_3/ai_97467594",
    abstract =  "Current standards for SOAP, WSDL, and UDDI have no explicit support for the versioning and deprecation of Web services. This article introduces a means for Web service versioning and deprecation that is lightweight and flexible, and requires minimal development effort."
}

