    year =      2005,
    volume =    37,
    number =    1,
    pages =     "29-41",
    topic =     "ac[0.8] acl[0.7] rbac[0.7] tbac[0.7] tmac[0.7]",
    uri =       "http://portal.acm.org/citation.cfm?id=1057977.1057979",
