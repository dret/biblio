}

@misc{offlinewebapps,
    author =    "Anne van Kesteren and Ian Hickson",
    title =     "Offline Web Applications",
    howpublished =  "World Wide Web Consortium, Note NOTE-offline-webapps-20080530",
    month =     may,
    year =      2008,
    topic =     "html[0.9]",
    uri =       "http://www.w3.org/TR/2008/NOTE-offline-webapps-20080530",
