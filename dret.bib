    month =     dec,
    year =      2006,
    topic =     "xmldsig[0.9]",
    uri =       "http://www.w3.org/TR/2006/NOTE-DSig-usage-20061220",
    abstract =  "This technical note describes how to use the XML Digital Signature Recommendation in a way consistent with the present (fall 2006) XML environment. In particular, this note takes into account the recent xml:id Version 1.0 and Canonical XML Version 1.1 Recommendations. This note suggests constraints on the use of XML Signature, and relies on extension points present in the XML Digital Signature Recommendation. This note does not override any aspect of that Recommendation."
}

@misc{xmldsigfilter,
    author =    "John M. Boyer and Merlin Hughes and Joseph M. Reagle",
    title =     "XML-Signature XPath Filter 2.0",
    howpublished =  "World Wide Web Consortium, Recommendation REC-xmldsig-filter2-20021108",
    month =     nov,
    year =      2002,
    topic =     "xmldsig[1] xpath1[0.7]",
    uri =       "http://www.w3.org/TR/2002/REC-xmldsig-filter2-20021108",
    abstract =  "XML Signature recommends a standard means for specifying information content to be digitally signed and for representing the resulting digital signatures in XML. Some applications require the ability to specify a subset of a given XML document as the information content to be signed. The XML Signature specification meets this requirement with the XPath transform. However, this transform can be difficult to implement efficiently with existing technologies. This specification defines a new XML Signature transform to facilitate the development of efficient document subsetting implementations that interoperate under similar performance profiles."
}

