@misc{soap12-0,
    author =    "Nilo Mitra",
    title =     "SOAP Version 1.2 Part 0: Primer",
    howpublished =  "World Wide Web Consortium, Recommendation REC-soap12-part0-20030624",
    month =     jun,
    year =      2003,
    topic =     "soap[0.9]",
    updates =   "soap12",
    uri =       "http://www.w3.org/TR/2003/REC-soap12-part0-20030624",
    abstract =  "SOAP Version 1.2 Part 0: Primer is a non-normative document intended to provide an easily understandable tutorial on the features of the SOAP Version 1.2 specifications. In particular, it describes the features through various usage scenarios, and is intended to complement the normative text contained in Part 1 and Part 2 of the SOAP 1.2 specifications."
}

@misc{soap12-0sec,
    author =    "Nilo Mitra and Yves Lafon",
    title =     "SOAP Version 1.2 Part 0: Primer (Second Edition)",
    howpublished =  "World Wide Web Consortium, Recommendation REC-soap12-part0-20070427",
    month =     apr,
    year =      2007,
    topic =     "soap[0.9] mtom[0.9] xop[0.9] rrshb[0.9]",
    updates =   "soap12-0",
    uri =       "http://www.w3.org/TR/2007/REC-soap12-part0-20070427",
