    title =     "Authoring Techniques for Device Independence",
    howpublished =  "World Wide Web Consortium, Note NOTE-di-atdi-20040218",
    month =     feb,
    year =      2004,
    uri =       "http://www.w3.org/TR/2004/NOTE-di-atdi-20040218"
