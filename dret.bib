@article{bie97,
    author =    "Michael Bieber and Fabio Vitali and Helen Ashman and V. Balasubramanian and Harri Oinas-Kukkonen",
    title =     "Fourth Generation Hypermedia: Some Missing Links for the World Wide Web",
    journal =   ijhcs,
    year =      1997,
    volume =    47,
